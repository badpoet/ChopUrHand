{
	"name": "ChopUrHand",
	"version": "3.0.0",
	"description": "Inhibition of visit some webpages in certain time.",
	"icons": { 
		"48": "icon.png",
		"128": "icon.png"
	},

	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "ChopUrHand"
	},
	"background": { "scripts": ["background.js"] },

	"permissions": [
		"http://*/*",
		"tabs",
		"alarms"
	],

	"manifest_version": 2
}
	
